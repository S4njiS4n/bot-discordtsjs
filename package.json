{
  "name": "botdiscordts",
  "version": "0.1.0",
  "description": "Projet Bot Discord TS",
  "main": "main.js",
  "repository": "https://github.com/S4njiS4n/bot-discordtsjs.git",
  "author": "Fhin",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^12.0.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-aqua": "^7.2.1"
  },
  "eslintConfig": {
    "extends": "aqua/node",
    "rules": {
      "linebreak-style":[
        "error",
        "windows"
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "indent": [
        "error",
        2
      ],
      "curly": "off"
    }
  }
}
